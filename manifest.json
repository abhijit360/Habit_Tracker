{
    "name": "Habit Tracker",
    "description": "Track your daily tasks and time spent on them!",
    "version": "1.0",
    "manifest_version": 3,
    "background" : {
        "service_worker" : "background.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": "images/logo.png"
    },
    "content_scripts" :[
      {
        "matches" : ["<all_urls>"],
        "js": ["content.js"]
      }
    ]

  }